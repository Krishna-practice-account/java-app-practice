pipeline {
    agent {
        label 'DevServer'
    }
    tools {
      maven 'mymaven'
    }
    environment {
  name = "krishna"
}

    stages {
        stage('Build') {
            steps {
                sh 'mvn clean package'
                echo "Hello ${name}, Build Successful!"
            }
            post {
                success {
                    archiveArtifacts artifacts: '**/target/*.jar'
                  
                }
            }
        }
    }
}
